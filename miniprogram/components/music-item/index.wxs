function allSongArtist(artists) {
  var res = ''
  if (artists && artists.constructor && artists.constructor == 'Array') {
    for (var i = 0; i < artists.length; i++) {
      if (res !== '') res += '/'
      res += artists[i].name
    }
  }
  return res
}

module.exports = {
  allSongArtist: allSongArtist,
}